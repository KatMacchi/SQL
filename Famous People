SELECT fullname as Full_Name, oscar_wins as 
   Oscar_Wins, title as Movie_Title
   FROM movies
   LEFT OUTER JOIN movies
   ON actors.id=cast_member_id
   WHERE fullname LIKE "Jennifer%" AND release_year < 2010;
